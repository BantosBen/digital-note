<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sanj.digote">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_d_note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_d_note_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.cloudActivities.CloudUpdateDetails"/>
        <activity android:name=".activities.cloudActivities.CloudAccountSetting" />
        <activity android:name=".activities.cloudActivities.CloudAddNote" />
        <activity android:name=".activities.cloudActivities.CloudHome" />
        <activity android:name=".activities.cloudActivities.CloudSinIn" />
        <activity android:name=".activities.cloudActivities.CloudSignUp" />
        <activity android:name=".activities.localActivities.Vault" />
        <activity android:name=".activities.localActivities.VaultLogin" />
        <activity android:name=".activities.localActivities.Settings" />
        <activity android:name=".activities.localActivities.AddNote" />
        <activity android:name=".activities.localActivities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>